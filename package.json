{
	"name": "@aicacia/streams",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"tauri": "tauri",
		"vite": "vite",
		"start": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"desktop": "tauri dev",
		"desktop.build": "tauri build",
		"test": "playwright test",
		"openapi-streams": "rm -rf src/lib/openapi/streams && openapi-generator-cli generate -i http://localhost:8080/swagger/doc.json -g typescript-fetch -o 'src/lib/openapi/streams' --type-mappings=DateTime=Date --additional-properties=modelPropertyNaming=snake_case,,supportsES6=true,withInterfaces=true",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"engine": {
		"node": ">=18"
	},
	"devDependencies": {
		"@openapitools/openapi-generator-cli": "^2.5.2",
		"@sveltejs/adapter-static": "^2.0.1",
