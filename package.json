		"@types/leaflet.markercluster": "^1.5.1",
		"@types/postcss-import": "^14.0.0",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"autoprefixer": "^10.4.14",
